source 'https://rubygems.org'

group :jekyll_plugins do
  gem 'jekyll'
  gem 'jekyll-feed'
  gem 'jekyll-redirect-from'
  gem 'jemoji'
  gem 'webrick', '~> 1.8'
end

gem 'github-pages'
gem 'connection_pool', '2.5.0'

# Lock rubyzip to a 2.x version range to avoid unexpected breakage
# when rubyzip 3.0 is released. Require at least 2.3.0 but < 3.0.
gem 'rubyzip', '>= 2.3.0', '< 3.0'
